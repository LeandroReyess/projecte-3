;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.digicore-16.test. root.digicore-16.test (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	  	 IN	NS	ns1.digicore-16.test.
ns1	 	 IN	A	192.168.4.16
server	 IN	A	192.168.4.16
dbserver IN	A	192.168.4.116
data	 IN	CNAME	dbserver.digicore-16.test.
